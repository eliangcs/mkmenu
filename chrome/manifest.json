{
  "name": "mkmenu",
  "version": "0.9.0",
  "description": "Enhances Google Chrome with marking menu.",
  "icons": { "32": "icon_32.png", "64": "icon_64.png" },
  "default_locale": "en",
  
  "background_page": "background.html",
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-1.6.4.min.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  //"key": "....",
  "options_page": "options.html",
  "permissions": ["tabs"]
}
